  "authors": [
    "SuperPro148"
  ],
  "contact": {
    "homepage": "https://superpro148.github.io/pages/mcmods.html",
    "sources": "https://github.com/SuperPro148/enchantviewer",
    "issues": "https://github.com/SuperPro148/enchantviewer/issues"
  },

  "license": "GPL-3.0-only",
  "icon": "assets/enchantviewer/icon.png",

  "environment": "client",
  "entrypoints": {
